using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Declare_Variables
{
    class Program
    {
        static void Main(string[] args)
        {
            byte a = 97;
            Console.WriteLine(a);
            sbyte b = -115;
            Console.WriteLine(b);
            short c = -10000;
            Console.WriteLine(c);
            ushort d = 52130;
            Console.WriteLine(d);
            int e = 4825932;
            Console.WriteLine(e);
        }
    }
}
