using System;


namespace Float_or_double
{
    class Program
    {
        static void Main()
        {
            float a = 12.345f;
            float b = 3456.091f;
            double c = 34.567839023;
            double d = 8923.1234857;
            Console.WriteLine("{0}\n{1}\n{2}\n{3}", a, b, c, d);
        }
    }
}
